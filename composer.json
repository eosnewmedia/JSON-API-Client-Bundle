{
  "name": "enm/json-api-client-bundle",
  "description": "Symfony integration for enm/json-api-client",
  "type": "symfony-bundle",
  "require": {
    "php": ">=7.2",
    "enm/json-api-client": "^4.0",
    "symfony/framework-bundle": "^5.0"
  },
  "suggest": {
    "kriswallsmith/buzz": "To use buzz as http client",
    "nyholm/psr7": "To use guzzle as http client"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Philipp Marien",
      "email": "marien@eosnewmedia.de"
    }
  ],
  "autoload": {
    "psr-4": {
      "Enm\\Bundle\\JsonApi\\Client\\": "src"
    }
  },
  "minimum-stability": "stable"
}
